<%- include("partials/head.ejs") %>
<%- include("partials/header.ejs") %>
<section class="main">
    <div><img class="head_img" src="/image/contactus.jpg" height="200" width="auto"></div>
    <di class="info-agent"></di>
    <h2 class="contactus-text">Properties for rent</h2>

    <div class="sell-container">
        <div class="sell-box">
            <h2>Why rent with Collected Homes?</h2>
            <p class="sell-text">By using our services, you get access to several brokerage firms but only have contact with one agent. It saves you time, energy and it is free of charge.</p>
               <p class="sell-text"> After a proper need analysis, we will present the best property among many of them for your specific wishes and budget.</p>
        </div>
        <div class="sell-box">
            <h2 class="form-head-text">Please enter your contact details and we will get back to you within 24 hours</h2>
            <form class="form-sell" method="post">
                <input type="text" id="name" name="name" placeholder="Your Name" required>
                <input type="email" id="email" name="email" placeholder="Your Email" required>
                <textarea id="message" name="message" rows="4" placeholder="Message" required></textarea>
                <button class="form-button" type="submit">Sen Message</button>
              </form>
        </div>
    </div>



    </section>

<%- include("partials/footer.ejs") %>